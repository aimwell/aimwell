<VirtualHost DOMAIN www.DOMAIN>
ServerName DOMAIN
ServerAdmin webmaster@DOMAIN
DocumentRoot /usr/local/etc/httpd/vhosts/DOMAIN/htdocs
TransferLog /usr/local/etc/httpd/vhosts/DOMAIN/logs/access_log
ErrorLog /usr/local/etc/httpd/vhosts/DOMAIN/logs/error_log
Alias /imanager /usr/local/etc/httpd/htdocs/imanager
<Directory /usr/local/etc/httpd/vhosts/DOMAIN/htdocs/urchin>
AddHandler cgi-script .cgi
DirectoryIndex index.html index.cgi
Options FollowSymLinks ExecCGI
</Directory>
</VirtualHost>
