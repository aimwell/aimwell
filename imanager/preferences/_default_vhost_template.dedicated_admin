<VirtualHost IP:PORT>
User vhost
Group vhost
ServerName DOMAIN
ServerAlias www.DOMAIN
ServerAdmin webmaster@DOMAIN
DocumentRoot /home/vhost/www/DOMAIN
ScriptAlias /cgi-bin/ /home/vhost/www/cgi-bin/
<Directory /home/vhost/www/cgi-bin>
AllowOverride None
Options ExecCGI
Order allow,deny
Allow from all
</Directory>
CustomLog  /usr/local/apache/logs/vhost/DOMAIN-access_log combined
ErrorLog /usr/local/apache/logs/vhost/DOMAIN-error_log
</VirtualHost>
