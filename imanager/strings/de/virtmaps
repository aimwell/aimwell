$VIRTMAPS_ERROR_VIRTUAL_FIELD_IS_BLANK = 
  "Das Feld für die virtuelle E-Mail Adresse ist leer.";
$VIRTMAPS_ERROR_REAL_FIELD_IS_BLANK = 
  "Das Feld für die echte E-Mail Adresse ist leer.";

$VIRTMAPS_ERROR_DUPLICATE_ADDITION = 
