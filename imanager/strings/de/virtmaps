$VIRTMAPS_NO_MAPPINGS_EXIST = 
  "Ihre Virtmap-Datei enthält keine virtuellen Adressmappings. Bitte
   benutzen Sie das 'Virtmap hinzufügen' Utility um neue virtuelle
   Adressmappings hinzuzufügen.";

$VIRTMAPS_SELECT_TITLE = "Virtmaps auswählen";
$VIRTMAPS_SELECT_HELP = 
  "Bitte benutzen Sie folgendes Formular um eine oder mehrere Virtmaps auszuwählen.";

$VIRTMAPS_OVERVIEW_HELP_TEXT = 
  "Spezifizieren Sie eine virtuelle E-Mail Adresse (linke Seite), die auf eine reale 
   E-Mail Adresse (rechte Seite) verweist. Die virtuelle Adresse kann eine vollständige
   E-Mail Adresse (benutzer\@domain.name) sein oder einfach nur ein Hostname (domain.name).
   Wenn Sie nur einen Hostnamen angeben werden E-Mails an diesen Hostnamen zu der
   angegebenen realen E-Mail Adresse geschickt, wenn kein bereits vorher angegebenes
   Virtuelles Mapping für diesen Domainnamen passt. Diese Funktion ermöglicht ein sog.
   'Catch-All' für E-Mails, die an irgendeine Adresse an diesem Hostnamen geschickt werden.";

$VIRTMAPS_EXAMPLES_HELP_TEXT_1 = "Beachten Sie folgende Beispiele:";
$VIRTMAPS_EXAMPLES_HELP_TEXT_2 = 
  "  Virtuelle E-Mail Adresse                Reale E-Mail Adresse
  ----------------------                ------------------
  webmaster\@servername.com            benutzer\@mail.dial-up.de
  benutzer\@servername.com                 localuser1
  servername.com                      benutzer\@beispiel.com
  # Bemerkung: \"Unbekannte Empfaenger\" -Mail an servername.com geht zu user\@beispiel.com
 
  webmaster\@www-server.com            localuser2
  benutzer\@www-server.com                 einbenutzer\@aol.com
  foobar\@www-server.com               andererbenutzer\@hotmail.com
  www-server.com                      localuser2";

$VIRTMAPS_VIRTUAL_EMAIL_ADDRESS = "Virtuelle E-Mail Adresse";
$VIRTMAPS_REAL_EMAIL_ADDRESS = "Reale E-Mail Adresse";

$VIRTMAPS_VIRTMAP_PLACEMENT = "Virtmap-Plazierung";
$VIRTMAPS_VIRTMAP_PLACEMENT_APPEND = "An das Dateiende anhängen";
$VIRTMAPS_VIRTMAP_PLACEMENT_INSERT = "nach __VIRTMAP__ einfügen";

