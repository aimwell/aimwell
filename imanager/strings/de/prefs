# mail manager preferences

$PREFS_MAILMANAGER_TEXT = "Einstellungen für Mail Manager";

$PREFS_MAILMANAGER_DENIED_TEXT = 
  "Ihr Benutzerprofil enthält keine Berechtigungen für 'mail'. Deshalb
   wird Ihnen der Zugriff auf die Einstellungen für den Mail Manager
   nicht gestattet.";

$PREFS_NUM_MESSAGES_TEXT = "Anzahl der angezeigten Nachrichten";
$PREFS_NUM_MESSAGES_HELP = 
  "Wieviele Nachrichten möchten Sie bei der Ansicht eines Mailordners 
   gleichzeitig angezeigt bekommen? Der Standardwert ist 25.";
$PREFS_NUM_MESSAGES_UNITS = "Nachrichten";

$PREFS_CONFIRM_MAIL_REMOVE_TEXT = "Löschen von E-Mails bestätigen";
$PREFS_CONFIRM_MAIL_REMOVE_HELP = 
  "Das Mail Manager Utility wird Sie auffordern das endgültige Löschen einer
   Nachricht zu bestätigen. Standardmäßig wird immer nach einer Bestätigung
   gefragt.";

$PREFS_DEFAULT_MAIL_FOLDER_TEXT = "Standardverzeichnis für Mailordner";
$PREFS_DEFAULT_MAIL_FOLDER_HELP = 
  "Ihr Standardverzeichnis für Mailordner legt fest, wo sich Ihre
   Mailordner befinden. Der Mail Manager erlaubt es Ihnen Ihren aktiven 
   Mailordner zu ändern, Nachrichten in einem angegebenen Mailordner
   zu speichern und ausgehende Nachrichten in einen angegebenen
   Mailordner zu kopieren -- es wird davon ausgegangen, dass jeder
   dieser Mailordner sich anfangs in Ihrem Standard-Mailordner-Verzeichnis
   befindet Die Verzeichnisse \"/Mail\" oder \"/mail\" sind gebräuchliche
   Spezifikationen für den Standard-Mailordner.";

$PREFS_MAIL_ATTACH_ELEMENTS_TEXT = "Anzahl der Formularelemente für Anlagen (Attachments)";
$PREFS_MAIL_ATTACH_ELEMENTS_HELP_NO_FTP_PRIVS = 
  "Bei der Erstellung von Nachrichten erlaubt es Ihnen das Mail Manager Utility
   lokale Dateien auf Ihrem Computer zu wählen die Sie an eine ausgehende
   Nachricht anhängen wollen. Diese Dateien werden mittels eines Formulars
   upgeloaded und dann an die Nachricht angehängt, bevor sie verschickt wird.
   Die Anzahl der Formularelemente, die im Mail Manager Formular zur Erstellung
   von Nachrichten angezeigt werden kann hier spezifiziert werden. der Standard
   ist 2.";
$PREFS_MAIL_ATTACH_ELEMENTS_HELP_FULL_PRIVS = 
  "Bei der Erstellung von Nachrichten erlaubt es Ihnen das Mail Manager Utility
   lokale Dateien auf Ihrm Computer oder auch Dateien auf diesem Server zu 
   spezifizieren, die Sie an eine ausgehende Nachricht anhängen möchten. Die 
   Dateien auf Ihrem Computer werden mittels eines Formulars upgeloaded und
   dann an die Nachricht angehängt, bevor sie verschickt wird. Die Anzahl der
   Formularelemente für jeden Typ von Anhang, die im Mail Manager beim Erstellen
   einer Nachricht angezeigt werden sollen, kann hier eingestellt werden. Der
   für jeden Typ ist 2.";
$PREFS_UPLOAD_ATTACH_ELEMENTS_UNITS = "Anhänge aus Formularelement(en) uploaden";
$PREFS_LOCAL_ATTACH_ELEMENTS_UNITS = "Formularelemente für Anhänge auf dem Server";

