##############################################################################
# security preferences

$PREFS_SECURITY_TEXT = "Sicherheits Einstellungen";

$PREFS_SECURITY_FORCE_HTTPS_TEXT = "Eine sichere Verbindung erzwingen";
$PREFS_SECURITY_FORCE_HTTPS_HELP = 
  "Sie können alle unverschlüsselten (nicht-SSL) Verbindungen umleiten auf eine
   SSL-verschlüsselte. Jedoch muss dafür SSL auf dem VPS account installiert sein.
   Die Grundeinstellung ist, das unverschlüsselte Verbindungen erlaubt sind.";
$PREFS_SECURITY_FORCE_HTTPS_YES = "Eine sichere Verbindung erzwingen";
$PREFS_SECURITY_FORCE_HTTPS_NO = "Nicht-SSL Verbindungen erlauben";

$PREFS_SECURITY_IP_CHECK_TEXT = 
  "Hostnamen Überprüfung aktivieren (IP Überprüfung)";
$PREFS_SECURITY_IP_CHECK_HELP = 
  "Eine der Authentifizierungen die vorgenommen werden, ist der Vergleich der
Hostnamen(oder IP Adressen) während aufeinanderfolgenden Sitzungen. Dieser Vergleich
verhindert, dass unerlaubte Parteien von anderen Rechnern aus Ihrem
Autthentifizierungs-Schlüssel nutzen, ausser von dem einen Rechner, an dem
die Sitzung gestartet wurde. Einige Internet Benutzer konnektieren jedoch von
einem proxy Server, bei welchem der Hostname oder die IP Adresse dynamisch ist.
Die Standardeinstellung ist, dass die Authentifizierungen über Hostname oder 
IP-Adressen immer gemacht werden.";
$PREFS_SECURITY_IP_CHECK_YES = "IP Überprüfung aktivieren";
$PREFS_SECURITY_IP_CHECK_NO = "IP Überprüfung deaktivieren";

##############################################################################
# file manager preferences

$PREFS_FILEMANAGER_TEXT = "Einstellungen für Dateimanager";

$PREFS_FILEMANAGER_DENIED_TEXT = 
  "Ihr benutzerprofil enthält keine Berechtigungen für FTP. Deshalb
   ist es Ihnen nicht gestattet, die Einstellungen für den Dateimanager zu benutzen.";

$PREFS_HIDE_ENTRIES_THAT_BEGIN_WITH_A_DOT_TEXT = 
  "Verzeichniseinträge, die mit einem Punkt (.) beginnen, verbergen.";
$PREFS_HIDE_ENTRIES_THAT_BEGIN_WITH_A_DOT_HELP = 
  "Verzeichnislisten im Dateimanager können Einträge enthalten, deren
   Namen mit einem Punkt (.) beginnen oder diese ignorieren. Dateien, die
   mit einem Punkt beginnen werden oft als 'versteckte Dateien' bezeichnet
   und werden typischerweise von Applikationen angelegt und gepflegt. 
   Standardmäßig werden Verzeichnisse, deren Namen mit einem Punkt
   beginnen nicht angezeigt.";

$PREFS_CONFIRM_FILE_REMOVE_TEXT = "Löschen von Dateien bestätigen";
$PREFS_CONFIRM_FILE_REMOVE_HELP = 
  "Das Utility zum Löschen von Dateien wird Sie auffordern das endgültige
   Löschen einer Datei oder eines Verzeichnisses zu bestätigen. Standardmäßig
   wird immer nach einer Bestätigung gefragt.";

$PREFS_CONFIRM_FILE_OVERWRITE_TEXT = "Überschreiben von Dateien bestätigen";
$PREFS_CONFIRM_FILE_OVERWRITE_HELP = 
  "Bestimmte Utilities des Dateimanagers werden Sie auffordern eine Aktion zu
   bestätigen, wenn das Ergebnis eine existierende Datei überschreiben wird.
   Standardmäßig wird immer nach einer Bestätigung gefragt.";
   
$PREFS_CONFIRM_DIR_CREATE_TEXT = "Erstellung von Verzeichnissen bestätigen";
$PREFS_CONFIRM_DIR_CREATE_HELP = 
  "Bestimmte Utilities des Dateimanagers werden Sie auffordern eine Aktion zu
   bestätigen, wenn das Ergebnis ein neues Verzeichnis anlegen wird. Standardmäßig
   wird immer nach einer Bestätigung gefragt.";

$PREFS_CHMOD_DEFAULT_OPTION_TEXT = "Standardoptionen für Berechtigungsänderungen";
$PREFS_CHMOD_DEFAULT_OPTION_HELP = 
  "Das Utility zur Änderung von Berechtigungen, die es Ihnen
   erlauben die Berechtigungen für gewählte Dateien oder Verzeichnisse zu ändern.
   Sie können wählen, was standardmäßig angezeigt wird.";
   
$PREFS_UPLOAD_FILE_ELEMENTS_TEXT = "Anzahl der Formularelemente für Dateiuploads";
$PREFS_UPLOAD_FILE_ELEMENTS_HELP = 
  "Das Utility zum Upload von Dateien erlaubt es Ihnen, lokale Dateien auf Ihrem
   Computer zu spezifizieren und mittels eines Formulars upzuloaden. Die Anzahl
   von Formularelementen für den Upload, die im Utility für Dateiuploads angezeigt 
   werden kann hier spezifiziert werden. Der Standard ist 4.";
$PREFS_UPLOAD_FILE_ELEMENTS_UNITS = "Formularelemente für Dateiuploads";

