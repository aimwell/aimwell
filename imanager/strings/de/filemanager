$FILEMANAGER_SOURCE_TARGET_IDENTICAL_ERROR = 
  "Die angegebene Zieldatei \"__TARGET__\" ist identisch mit 
   der Quelldatei \"__SOURCE__\", die Sie gewählt haben!";

$FILEMANAGER_NO_CLOBBER_ERROR = 
  "Die gewählte Quelldatei \"__SOURCE__\" ist ein Verzeichnis.
   Trotzdem ist die angegebene Zieldatei \"__TARGET__\" eine existierende Datei.  Sie können eine existierende Datei nicht mit einem Verzeichnis verschmelzen. Bitte
   löschen Sie zuerst die existierende Datei.";

$FILEMANAGER_FILE_IN_TARGET_PATH_ERROR = 
  "Der gewählte Zielpfad \"__TARGET__\" enthält eine Unterverzeichnis-Komponente \"__SUBPATH__\", die
   eine existierende Datei ist.  Um Ihre Anforderung zu erfüllen, muessen die existierende Datei erst löschen.";

$FILEMANAGER_FILETYPE_INVALID_ERROR = 
  "Die gewählte Quelldatei \"__SOURCE__\" ist ein __LCTYPE__.
   Das ist ein ungültiger Dateityp für dieses Utility.";

$FILEMANAGER_PERMISSION_DENIED = 
  "Die Berechtigung zum Zugriff/zur Änderung auf den Pfad  \"__PATH__\" wurde verweigert."; 

$FILEMANAGER_EMPTY_FIELD_ERROR = 
  "Sie haben keinen Wert für __NAME__ angegeben."; 

