# edit file strings

$FILEMANAGER_ACTIONS_EDIT = "Datei editieren";
$FILEMANAGER_ACTIONS_EDIT_SAVE = "Geänderte Datei speichern";
$FILEMANAGER_ACTIONS_EDIT_CANCEL = "Abbrechen und Änderungen verwerfen";
$FILEMANAGER_ACTIONS_EDIT_TEXT = 
  "Benutzen Sie untenstehendes Formular um die Datei \"__FILE__\" zu Ändern. Wenn Sie 
   die Änderungen, die Sie an der Datei durchführen speichern möchten, klicken Sie auf den 
   \"$FILEMANAGER_ACTIONS_EDIT_SAVE\" Button.  Wenn Sie sich entschließen abzubrechen
   und alle Änderungen zu verwerfen, steht Ihnen ebenfalls einen entsprechender Button zur
   Verfügung.";

$FILEMANAGER_ACTIONS_EDIT_WRAP_OFF = "Zeilenumbrüche ausschalten";
$FILEMANAGER_ACTIONS_EDIT_WRAP_VIRTUAL = "Sanfte Zeilenumbrüche";
$FILEMANAGER_ACTIONS_EDIT_WRAP_PHYSICAL = "Harte Zeilenumbrüche";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TITLE =
  "Hilfe zu den Zeilenumbruchs-Optionen";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT =
  "Zeilenumbruchs-Optionen legen fest, wie Zeilenumbrüche im Dateieditor
   behandelt werden. 'Zeilenumbr\xfcche ausschalten' schaltet Zeilenumbrüche komplett ab,
   d.h. die einzigen Zeichen für eine neue Zeile sind die, die ausdrücklich
   eingetippt oder in die Datei eingefügt werden.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT_SOFT =
  "'Sanfte Zeilenumbr\xfcche' bedingt virtuelle Zeilenumbrüche, d.h. Zeichen für Zeilenwechsel
   werden eingeführt um sicher zu sein, dass der Text in den Dateieditor Platz hat,
   aber diese Zeichen sind nicht in den Daten enthalten, wenn die editierte Datei
   gespeichert wird.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT_HARD =
  "'Harte Zeilenumbr\xfcche', ergibt Zeilenumbrüche wie bei WRAP='soft',
   mit dem Unterschied, daß Zeichen für Zeilenwechsel erhalten
   bleiben, wenn die geänderten Daten gespeichert werden."; 

$FILEMANAGER_ACTIONS_EDIT_QUOTA_ERROR = 
  "Das Speichern der Datei \"__FILE__\" konnte wegen Quota-Beschränkungen
   nicht erfolgreich durchgeführt werden. Sie müssen entweder freien Speicherplatz
   durch das Löschen nicht benötigter Dateien schaffen oder mehr Speicherplatz
   von Ihrem Serveradministrator anfordern.";
$FILEMANAGER_ACTIONS_EDIT_SAVE_TEXT = 
  "Ihre Änderungen wurden erfolgreich gespeichert; unten sehen Sie
   das Datum der letzten Änderung.";
$FILEMANAGER_ACTIONS_EDIT_SAVE_HTTPD_CONF_TEXT = 
  "Ihre Änderungen an der Datei httpd.conf wurden erfolgreich gespeichert. Ihr
   Webserver sollte jetzt neu starten. Bitte warten Sie einige Sekunden bevor 
   Sie $SOFTWARE_TITLE weiter benutzen.";
$FILEMANAGER_ACTIONS_EDIT_CANCEL_TEXT = 
  "Das Editieren der Datei wurde abgebrochen und sämtliche vorgenommenen Änderungen
   an der Datei wurden verworfen.";

