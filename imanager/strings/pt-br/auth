$AUTH_TITLE = "Autorização Requerida";

$AUTH_LOGINID    = "Login ID";
$AUTH_PASSWORD = "Senha";

$AUTH_SETCOOKIE = "Lembrar minha Senha";
$AUTH_SETCOOKIE_HELP_TEXT = 
  "O recurso de memorização de senhas, quando selecionado, dispara um
   mecanismo no servidor que envia ao seu navegador dados de
   autenticação e solicita que os mesmos sejam armazenados temporariamente.
   Os dados, ou 'cookies' (como são popularmente chamados), são reenviados
   ao servidor pelo navegador a cada solicitação subseqüente efetuada.
   Esse método de troca de dados não é incomum e é preferido em lugar
   do padrão que seria passar os dados de autenticação embutidos
   em elementos ocultos de formulário ou totalmente a vista como
   argumentos em URLs."; 

$AUTH_UNKNOWNLOGIN = "O Login ID que você forneceu não é conhecido.";
$AUTH_BADPASSWORD = "A Senha fornecida não está correta.";
$AUTH_LOGGEDOUT = "Você foi desconectado do $SOFTWARE_TITLE.";
$AUTH_BADHOST = 
  "O nome do host 'hostname' autorizado mudou.  Por favor identifique-se novamente.";
$AUTH_EXPIRED = 
  "Suas credenciais de autenticação são antigas.  Por favor identifique-se novamente.";

