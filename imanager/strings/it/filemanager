# edit file strings

$FILEMANAGER_ACTIONS_EDIT = "Edita File";
$FILEMANAGER_ACTIONS_EDIT_SAVE = "Memorizza File Editato";
$FILEMANAGER_ACTIONS_EDIT_CANCEL = "Annulla e abbandona le modifiche";
$FILEMANAGER_ACTIONS_EDIT_TEXT = 
  "Utilizza il modulo qui sotto per editare il file, \"__FILE__\".
   Se desideri memorizzare le modifiche che apporti al file,
   seleziona il tasto di \"$FILEMANAGER_ACTIONS_EDIT_SAVE\".  Se
   vuoi annullare e abbandonare, utilizza il tasto apposito.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_OFF = "Word-wrapping Spento";
$FILEMANAGER_ACTIONS_EDIT_WRAP_VIRTUAL = "Word-wrapping Leggero";
$FILEMANAGER_ACTIONS_EDIT_WRAP_PHYSICAL = "Word-wrapping Duro";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TITLE =
  "Aiuto sulle opzioni di Word Wrap";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT =
  "Le opzioni di word-wrapping specificano come word-wrapping è configurato
   all'interno dell'elemento da editare.  WRAP='Spento' disabilita il 
   word-wrapping completamente, cioè i caratteri di una nuova riga sono
   quelli scritti o inclusi esplicitamente nel file.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT_SOFT =
  "WRAP='leggero' abilita il word-wrapping virtuale, cioè i caratteri di 
   una nuova riga sono introdotti per accertarsi che le misure del testo
   all'interno dell'elemento, ma questi caratteri non
   saranno inclusi con i dati quando il file editato verra' memorizzato.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT_HARD =
  "WRAP='duro' e' il word-wrapping equivalente a WRAP='leggero',
   ma in questo caso i caratteri di nuova riga supplementari dovrebbero
   essere inclusi con i dati quando il file viene memorizzato.";

$FILEMANAGER_ACTIONS_EDIT_QUOTA_ERROR = 
  "La tua richiesta di memorizzare \"__FILE__\" non puo' essere completata a 
   causa dello spazio disponibile.
   Devi liberare spazio disco  rimuovendo file non
   necessari o richiedere spaziodisco supplementare al
   tuo amministratore di sistema.";
$FILEMANAGER_ACTIONS_EDIT_SAVE_TEXT = 
  "Le modifiche sono state memorizzate con successo; nota la data
   di modifica del file qui sotto.";
$FILEMANAGER_ACTIONS_EDIT_SAVE_HTTPD_CONF_TEXT = 
  "Le tue modifiche al file httpd.conf sono state memorizzate con successo.  Il
   tuo web server dovrebbe ora riavviare.  Attendi alcuni secondi
   prima di continuare nell'utilizzo di  $SOFTWARE_TITLE.";
$FILEMANAGER_ACTIONS_EDIT_CANCEL_TEXT = 
  "La tua richiesta di editare il file è stata annullata e tutte le modifiche
   che hai apportato al file sono state annullate.";

