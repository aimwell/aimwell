# some generic error strings

$FILEMANAGER_DENIED_TITLE = "$SOFTWARE_TITLE: Gestione File - Accesso negato";
$FILEMANAGER_DENIED_TEXT = 
  "Il tuo profilo non include i privilegi 'ftp'.  Di conseguenza, non
   puoi accedere ai programmi della Gestione File.";

$FILEMANAGER_HOMEDIR_INVALID_TITLE = 
  "$SOFTWARE_TITLE: Gestione File - Directory Principale non valida";
$FILEMANAGER_HOMEDIR_INVALID_TEXT = 
  "La directory principale del tuo profilo sembra non esistere.  Di 
   conseguenza, non potrai accedere ai programmi della Gestione
   File.";

$FILEMANAGER_NOTFOUND_TITLE = 
  "$SOFTWARE_TITLE: Gestione File - Risorsa non Trovata";
$FILEMANAGER_NOTFOUND_TEXT = 
  "E' strano...  il file che hai selezionato sembra non esistere!";

$FILEMANAGER_RESOURCE_ERROR_TITLE = 
  "$SOFTWARE_TITLE: Gestione File - Errore di Risorsa";
$FILEMANAGER_RESOURCE_ERROR_TEXT = 
  "Un errore e' accaduto mentre tentavi di eseguire una comando di
   __RESOURCE__.  Una descrizione dell'errore è riportata qui sotto:";

$FILEMANAGER_SOURCE_TARGET_IDENTICAL_ERROR = 
  "Il file di destinazione \"__TARGET__\" che hai specificato è 
   identico al file di origine \"__SOURCE__\" che hai
   selezionato!  ops!";

$FILEMANAGER_NO_CLOBBER_ERROR = 
  "Il file di origine \"__SOURCE__\" che hai selezionato è una directory.
   Il file di destinazione \"__TARGET__\" che hai specificato è un
   file.  Non puoi rimettere un file in una directory.
   Prego rimuovi il file prima."; 

$FILEMANAGER_FILE_IN_TARGET_PATH_ERROR = 
  "Il luogo di destinazione \"__TARGET__\" che hai specificato include un 
   componente \"__SUBPATH__\" che è un file.  Per completare la
   tua richiesta, dovrei rimuovere il file prima.";

$FILEMANAGER_FILETYPE_INVALID_ERROR = 
  "Il file di origine \"__SOURCE__\" che hai selezionato è un __LCTYPE__.
   Questo non è un tipo valido per questo programma.";

$FILEMANAGER_PERMISSION_DENIED = 
  "Permesso per accedere/modificare \"__PATH__\" negato.";

$FILEMANAGER_EMPTY_FIELD_ERROR = 
  "Non hai dato un valore per __NAME__.";

