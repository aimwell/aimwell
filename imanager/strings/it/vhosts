$VHOSTS_HOSTNAMES = "Nomi dei Domini Virtuali";
$VHOSTS_SERVERNAME = "ServerName";
$VHOSTS_SERVERADMIN = "ServerAdmin";
$VHOSTS_DOCUMENTROOT = "DocumentRoot";
$VHOSTS_TRANSFERLOG = "TransferLog";
$VHOSTS_ERRORLOG = "ErrorLog";
$VHOSTS_OTHEROPTIONS = "Altre Opzioni";
$VHOSTS_SCRIPTALIAS = "ScriptAlias";

$VHOSTS_NO_HOSTS_EXIST = 
  "Sembra che il tuo file httpd.conf non includa alcuna definizione
   di domini virtuali.  Prego usa lo strumento 'Aggiungi Dominio Virtuale'
   per aggiungere dei nuovi domini virtuali.";

$VHOSTS_SELECT_TITLE = "Seleziona Domini Virtuali";
$VHOSTS_SELECT_HELP = 
  "Prego usa il modulo seguente per selezionare uno o più domini virtuali.";

$VHOSTS_OVERVIEW_HELP_TEXT = "Esempio di configurazione.";

$VHOSTS_OVERVIEW_EXAMPLE_TEXT =
  "  &lt;VirtualHost some-domain.name www.some-domain.name&gt;
    ServerName www.some-domain.name
    ServerAdmin webmaster\@some-domain.name
    DocumentRoot /usr/local/etc/httpd/vhosts/some-domain.name/htdocs
    ScriptAlias /cgi-bin/ /usr/local/etc/httpd/vhosts/some-domain.name/cgi-bin
    TransferLog /usr/local/etc/httpd/vhosts/some-domain.name/logs/access_log
    ErrorLog /usr/local/etc/httpd/vhosts/some-domain.name/logs/error_log
  &lt;/VirtualHost&gt;";

$VHOSTS_HOSTNAMES_HELP_TEXT = 
  "Il Domino Virtuale riportato nel <VirtualHost> tag 
   corrisponde al nome dominio che definisce il virtual host. 
   Se desideri inserire piu' di un Dominio Virtuale separa ogni 
   hostname con uno spazio (es: nomedominio.com  www.nomedominio.com).";
$VHOSTS_SERVERNAME_HELP_TEXT = 
  "ServerName imposta l'hostname per il nuovo dominio virtuale.";
$VHOSTS_SERVERADMIN_HELP_TEXT = 
  "ServerAdmin definisce l'indirizzo e-mail per i messaggi d'errore.";
$VHOSTS_DOCUMENTROOT_HELP_TEXT = 
   "DocumentRoot definisce e crea la directory root per il Dominio 
   Virtuale, che verra' mostrata quando il Domino Virtuale ricevera' 
   una richesta. Se la directory indicata non esiste verra' creata 
   secondo il percorso indicato.  E' consigliabile non utilizzare 
   un trailing slash ('/') alla fine del percorso.";
$VHOSTS_SCRIPTALIAS_HELP_TEXT = 
  "ScriptAlias definisce la directory CGI che verra' eseguita dal 
   webserver per il tuo dominio virtuale. Considera attentamente la 
   possibilita' di permettere l'utilizzo di script CGI; questi script 
   hanno pieno accesso alla struttura del tuo Virtual Server. Se la 
   directory indicata non esiste verra' creata secondo il percorso indicato.";
$VHOSTS_TRANSFERLOG_HELP_TEXT = 
  "La directory TransferLog indica e crea la directory dove verrano 
   registrate le richieste per il tuo Dominio Virtuale.";
$VHOSTS_ERRORLOG_HELP_TEXT = 
  "La directory ErrorLog indica e crea la directory dove verrano 
   registrate le richieste per il tuo Dominio Virtuale che hanno 
   riscontrato un errore.";

$VHOSTS_VHOST_PLACEMENT = "Posizione del Dominio Virtuale";
$VHOSTS_VHOST_PLACEMENT_APPEND = "metti alla fine del file";
$VHOSTS_VHOST_PLACEMENT_INSERT = "inserisci dopo __VHOST__";

$VHOSTS_ADD_HELP_TEXT = 
  "Prego usa il modulo seguente per aggiungere dei nuovi domini virtuali.";
$VHOSTS_ADD_SUBMIT_TEXT = $SUBMIT_STRING;

$VHOSTS_EDIT_HELP_TEXT = 
  "Prego usa il modulo seguente per fare cambiamenti ai domini virtuali
   selezionati.";
$VHOSTS_EDIT_SUBMIT_TEXT = "Invia Cambiamenti";

$VHOSTS_REMOVE_HELP_TEXT = 
  "Sei sicuro che vuoi eliminare il dominio virtuale elencato sotto?";
$VHOSTS_REMOVE_SUBMIT_TEXT = "Sì, elimina il dominio virtuale sopra";

$VHOSTS_SORT_BY_HOST = "Ordina per Hostname";
