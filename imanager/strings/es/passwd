$PASSWD_WARNING_DUPLICATE_LOGIN =
  "Advertencia: duplicados de __LOGIN__ encontrados en el archivo de contrasenas..."; 

$PASSWD_WARNING_ROOT_UID =
  "Alvertencia: usuario (root) tiene una identificacion de usuario de __UID__ la cual no concuerda 
   con la identificacion esperada de 0.";
 
$PASSWD_WARNING_ROOT_GID =
  "Advertencia: usuario (root) tienen una identificacion de grupo de __UID__ la cual no concuerda
   con la identificaion esperada de 0.";

$PASSWD_WARNING_NONROOT_UID =
  "Advertencia: usuario (__LOGIN__) tiene una identification de usuario de __UID__ la cual no concuerda con 
   la identificacion espereara de __EUID__.";

$PASSWD_WARNING_NONROOT_GID =
  "Advertencia: usuario (__LOGIN__) tiene una identificacion de grupo de __GID__ la cual no concuerda
   con la identification de grupo de __EGID__.";

$PASSWD_RESOURCE_ERROR_TITLE = "$SOFTWARE_TITLE: Error de recurso en el Archivo de contrasena"; 
$PASSWD_RESOURCE_ERROR_TEXT = 
  "An error occurred while attempting to execute a password command.
   A description of the error is given below:";
  "Un error ocurrio tratando de ejecutar un comando de contrasena.
   En seguida hay una descripcion del error:";

##############################################################################
# eof

1;

