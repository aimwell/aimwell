##############################################################################
# some generic error strings

$FILEMANAGER_DENIED_TITLE = "$SOFTWARE_TITLE: Cordinador de Archivos - Acceso Negado";
$FILEMANAGER_DENIED_TEXT = 
  "Tu perfil no incluye privilegios de 'ftp'. Por esta razon
   no tendras acceso a las herramientas del Cordinador de Archivos."; 

$FILEMANAGER_HOMEDIR_INVALID_TITLE = 
  "$SOFTWARE_TITLE: Cordinador de Archivos - Directorio Principal Invalido";
$FILEMANAGER_HOMEDIR_INVALID_TEXT = 
  "Al parece el directorio principal de tu perfil no existe. Por esta
   razon no tendras acceso a las herramientas del Cordinador de Archivos.";

$FILEMANAGER_NOTFOUND_TITLE = 
  "$SOFTWARE_TITLE: Cordinador de Archivos - Recurso Perdido";
$FILEMANAGER_NOTFOUND_TEXT = 
  "Que raro... parece que el archivo que seleccionaste no existe!";

$FILEMANAGER_RESOURCE_ERROR_TITLE = 
  "$SOFTWARE_TITLE: Cordinador de Archivos - Error de Recurso."; 
$FILEMANAGER_RESOURCE_ERROR_TEXT = 
  "Un error ocurio tratando de ejecutar una order __RESOURCE__ .
   En seguida es dada una descripcion del error.";

$FILEMANAGER_SOURCE_TARGET_IDENTICAL_ERROR = 
  "El archivo de destinacion \"__TARGET__\" es identico al
   archivo de origen \"__SOURCE__\".";

$FILEMANAGER_NO_CLOBBER_ERROR = 
  "El archivo de origen \"__SOURCE__\" que seleccionaste es un directorio.
   Sin embargo, el archivo de destinacion \"__TARGET__\" que especificaste
   es un archivo que ya existe. Tu no puedes borrar un archivo que ya 
   existe con un directorio. Borra por favor el archivo que ya existe.";

$FILEMANAGER_FILE_IN_TARGET_PATH_ERROR = 
  "El lugar del archivo de destinacion \"__TARGET__\" que especificaste
   uncluye un lugar \"__SUBPATH__\" el cual es un archivo que ya existe.
   Para poder terminar tu peticion, tendras que borrar el archivo que
   ya existe.";

$FILEMANAGER_FILETYPE_INVALID_ERROR = 
  "El archivo de origen \"__SOURCE__\" que has seleccionado es de tipo __LCTYPE__. 
   Este tipo de archivo es invalido para esa herramienta.";

$FILEMANAGER_PERMISSION_DENIED = 
  "Permiso para entrar/modificar al lugar \"__PATH__\" no es permitido.";

$FILEMANAGER_EMPTY_FIELD_ERROR = 
  "Se te olvido poner un valor para __NAME__.";

