$ALIASES_NO_MAPPINGS_EXIST = 
  "It appears that your aliases file does not include any alias definitions.
   Please use the 'Add Alias' utility to add new aliases.";

$ALIASES_SELECT_TITLE = "Select Aliases";
$ALIASES_SELECT_HELP = 
  "Please use the following form to select one or more aliases.";

$ALIASES_OVERVIEW_HELP_TEXT = 
  "An E-mail Alias is simply a forwarding e-mail address.  Each e-mail
   alias you specify simply forwards e-mail on to the Alias Definition.
   The alias definition can simply be one or more e-mail addresses 
   (multiple addresses must be separated by commas), or it can be a 
   complex definition that directs your mail service to deliver a 
   message to a mailing list or a program such as an autoresponder.";

$ALIASES_EXAMPLES_HELP_TEXT_1 = "Consider the following examples:";
$ALIASES_EXAMPLES_HELP_TEXT_2 = 
  "  E-mail Alias                   Alias Definition
  ----------------------         ----------------------------
  webmaster                      localuser1
  sales                          localuser2, localuser3
  biff                           someuser\@aol.com
  staff                          :include:/etc/mail/staff_address.list
  info                           webmaster, \"|/usr/bin/autoreply -f webmaster -m /etc/mail/faq_message.txt -a info\"";

$ALIASES_ALIAS_NAME = "E-mail Alias";
$ALIASES_ALIAS_VALUE = "Alias Definition";

$ALIASES_ALIAS_PLACEMENT = "Alias Placement";
