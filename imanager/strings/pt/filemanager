$FILEMANAGER_DENIED_TEXT = 
  "O seu perfil de utilizador não contém privilégios de acesso via 'ftp', portanto,
   não será possível a utilização do Gerenciador de Arquivos.";

$FILEMANAGER_HOMEDIR_INVALID_TITLE = 
  "$SOFTWARE_TITLE: Gerenciador de Arquivos - Directório Principal Inválido";
