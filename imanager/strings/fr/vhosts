$VHOSTS_HOSTNAMES = "Nom(s) d'Hôte(s)";
$VHOSTS_SERVERNAME = "Nom de Serveur";
$VHOSTS_SERVERADMIN = "Administrateur de Système";
$VHOSTS_DOCUMENTROOT = "Racine de Document";
$VHOSTS_SCRIPTALIAS = "Script d'Alias";
$VHOSTS_TRANSFERLOG = "Log de Transfert";
$VHOSTS_ERRORLOG = "Journal des Erreurs";
$VHOSTS_OTHEROPTIONS = "Autres Directives (une par ligne)";

$VHOSTS_NO_HOSTS_EXIST = 
  "Il semble que votre fichier de configuration (httpd.conf) n'inclut 
   pas de définitions d'hôtes virtuels.  Veuillez utiliser l'utilitaire 
   'Ajouter Hôte Virtuel' afin d'ajouter de nouveaux hôtes.";

$VHOSTS_SELECT_TITLE = "Sélectionner Hôtes Virtuels";
$VHOSTS_SELECT_HELP = 
  "Veuillez utiliser la formulaire suivante à sélectionner un ou plusieurs hôtes virtuels.";

$VHOSTS_OVERVIEW_HELP_TEXT = 
  "le sous-hébergement virtuel vous permet de supporter plusieurs noms de
   domaines pouvant résoudre vers leur propre sous-répertoires tout en
   partageant le même adresse IP.  Autrement dit, vous pouvez
   héberger 'domaine1.name' et 'domaine2.name' sur le même serveur, chaque nom
   avec son propre site.  Chaque sous hôte virtuel est défini en utilisant le
   label &lt;VirtualHost&gt;  dans votre fichier de configuration
   (httpd.conf). Considerer l'exemple suivant:";

$VHOSTS_OVERVIEW_EXAMPLE_TEXT =
  "  &lt;VirtualHost domaine1.name www.domaine2.name&gt;
    ServerName www.domaine1.name
    ServerAdmin webmaster\@domaine1.name
    DocumentRoot /usr/local/etc/httpd/vhosts/domaine1.name/htdocs
    ScriptAlias /cgi-bin/ /usr/local/etc/httpd/vhosts/domaine1.name/cgi-bin
    TransferLog /usr/local/etc/httpd/vhosts/domaine1.name/logs/access_log
    ErrorLog /usr/local/etc/httpd/vhosts/domaine1.name/logs/error_log
  &lt;/VirtualHost&gt;";

$VHOSTS_HOSTNAMES_HELP_TEXT =
  "Les noms d'Hôtes Virtuels placés dans le label <VirtualHost>,
   sont des noms de domaines qui seront mappés dans la définition d'hôte
   virtuel.  Si plusieurs noms d'hôtes sont présents, séparer chaque nom avec
   un espace.";
$VHOSTS_SERVERNAME_HELP_TEXT =
  "'Nom de Serveur' definie le nom du seveur virtuel.";
$VHOSTS_SERVERADMIN_HELP_TEXT =
  "'Nom d'administrateur Serveur' met par defaut cette adresse e-mail qui sera renvoyée au client en cas d'erreur rencontre par le serveur.";
$VHOSTS_DOCUMENTROOT_HELP_TEXT =
  "'Racine' designe le répertoire 'racine' a partir duquel le seveur web  montrera les fichiers.  Puisque la valeur de la 'Racine doit pointer sur un
répertoire valide et pré-existant sur votre Serveur Virtuel, un nouveau répertoire sera créé si vous spécifiez un chemin de 'Racine' qui n'existe pas.  Merci de bien vouloir éviter l'utilisation d'une barre oblique ('/') lorsque vous spécifiez votre répertoire de 'racine'.";
$VHOSTS_SCRIPTALIAS_HELP_TEXT =
  "'Script d'Alias' marque le répertoire cible contenant les scripts CGI qui seront exécutés par le serveur.  Veuillez considerer attentivement votre décision de permettre à un utilsateur sous-hébergé l'aptitude de pouvoir exécuter des scripts par le serveur;  tous les scripts CGI (s'ils résident dans un répertoire sous-hébergé ou non) ont l'autorisation au répertoire entier de votre Serveur Virtuel.  Si vous spécifiéz un chemin de l'Alias de Script qui n'existe pas, un nouveau répertoire sera créé pour vous.";
$VHOSTS_TRANSFERLOG_HELP_TEXT =
  "'Log de Transfert' definit le nom du fichier dans lequel le serveur enregistrera toutes les demandes d'accès au serveur.";
$VHOSTS_ERRORLOG_HELP_TEXT =
  "'Journal des Erreurs' definit le nom du fichier dans lequel le serveur
enregistrera des données concernant les erreurs rencontrées par le serveur pendant les requêtes de traitements.";

$VHOSTS_VHOST_PLACEMENT = "Placement dans httpd.conf";
$VHOSTS_VHOST_PLACEMENT_APPEND = "ajouter au fin de fichier";
$VHOSTS_VHOST_PLACEMENT_INSERT = "insérer aprés __VHOST__";

$VHOSTS_ADD_HELP_TEXT = 
  "Veuillez utiliser le formulaire ci-dessous afin d'ajouter nouvelles définitions d'hôtes.";
$VHOSTS_ADD_SUBMIT_TEXT = $SUBMIT_STRING;

$VHOSTS_EDIT_HELP_TEXT = 
  "Veuillez utiliser le formulaire ci-dessous afin de modifier les définitions d'hôtes sélectionnés.";
$VHOSTS_EDIT_SUBMIT_TEXT = "Faire les changements";

$VHOSTS_REMOVE_HELP_TEXT = 
