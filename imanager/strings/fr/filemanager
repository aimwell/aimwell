# edit file strings

$FILEMANAGER_ACTIONS_EDIT = "Modifier Fichier";
$FILEMANAGER_ACTIONS_EDIT_SAVE = "Enregistrer Fichier Modifié";
$FILEMANAGER_ACTIONS_EDIT_CANCEL = "Annuler et jeter les Modifications";
$FILEMANAGER_ACTIONS_EDIT_TEXT = 
  "Utilisez la formulaire ci-dessous afin de modifier le fichier, \"__FILE__\".
   Si vous voulez enregistrer les modifications que vous faites au fichier,
   selectionnez le bouton \"$FILEMANAGER_ACTIONS_EDIT_SAVE\".  Si vous décidez
   d'annuler et jeter vos modifications, un bouton est aussi disponible.";

$FILEMANAGER_ACTIONS_EDIT_WRAP_OFF = "Utiliser 'Retour à la Ligne Automatique";
$FILEMANAGER_ACTIONS_EDIT_WRAP_VIRTUAL = "'Retour à la Ligne Automatique' douce";
$FILEMANAGER_ACTIONS_EDIT_WRAP_PHYSICAL = "'Retour à la Ligne Automatique' dure";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TITLE =
  "Options d'aides de 'Retour à la Ligne Automatique'.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT =
  "Les options de 'Retour à la Ligne Automatique' spécifient comment le
   manipuler dans l'élément 'modifier fichier'.  WRAP='éteint'
   désactives 'Retour à la Ligne Automatique' complètement, i.e. les seules
   caractères nouvelles-lignes sont ceux qui ont tapés explicitement ou inclus
   avec le fichier.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT_SOFT =
  "WRAP='doux' fait 'Retour à la Ligne Automatique' virtuel, i.e. caractères de
   nouvelles-lignes sont présentés pour s'assurer que le texte est placé dans
   l'élément 'modifier fichier', mais ces caractères ne sont pas inclus avec
   l'information lorsque la formulaire du fichier modifié est soumis.";
$FILEMANAGER_ACTIONS_EDIT_WRAP_HELP_TEXT_HARD =
  "WRAP='dur', devrait faire 'Retour à la Ligne Automatique' équivalent à
   WRAP='doux', mais dans ce cas, les nouvelles-lignes caractères
   supplémentaires devraient être inclues avec l'information lorsque la
   formulaire du fichier modifié est soumis."; 

$FILEMANAGER_ACTIONS_EDIT_QUOTA_ERROR = 
  "Votre demande à enregister \"__FILE__\" ne pouvait pas être complétée avec
   succès à cause de restrictions de quota.  Vous devez libérer de l'espace en
   supprimant les fichiers dont vous n'avez pas besoin -OU- Vous devez demander
   davantage d'espace de disque à votre administrateur de serveur.";
$FILEMANAGER_ACTIONS_EDIT_SAVE_TEXT = 
  "Vos modifications ont été enregistrées avec succès; noter les modifications
   de date du fichier récent, affichées ci-dessous.";
$FILEMANAGER_ACTIONS_EDIT_SAVE_HTTPD_CONF_TEXT = 
  "Vos modifications au httpd.conf ont été enregistrées.  Votre serveur Web
   devrait être en ligne maintenant.  Veuillez patienter quelques secondes
   avant de continuer à utiliser $SOFTWARE_TITLE.";
$FILEMANAGER_ACTIONS_EDIT_CANCEL_TEXT = 
  "Votre demande 'modifier fichier' a été annulée et toutes modifications que
   vous avez fait au fichier a été jeté.";

