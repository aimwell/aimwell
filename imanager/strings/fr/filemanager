##############################################################################
# some generic error strings

$FILEMANAGER_DENIED_TITLE = "$SOFTWARE_TITLE: Gestionnaire de Fichiers - Accés Refusé";
$FILEMANAGER_DENIED_TEXT = 
  "Votre nom d'utilisateur n'inclut pas de privilèges 'ftp'. Vous n'êtes donc pas autorisé à accéder aux outils de Gestionnaire de Fichiers.";

$FILEMANAGER_HOMEDIR_INVALID_TITLE = 
  "$SOFTWARE_TITLE: Gestionnaire de Fichiers - Répertoire Personnel non valide";
$FILEMANAGER_HOMEDIR_INVALID_TEXT = 
  "Le répertoire personnel de votre nom d'utilisateur n'existe pas.  
   Vous n'êtes donc pas autorisé à accéder aux outils de Gestionnaire de Fichiers.";

$FILEMANAGER_NOTFOUND_TITLE = 
  "$SOFTWARE_TITLE: Gestionnaire de Fichiers - Ressource n'est pas trouvée";
$FILEMANAGER_NOTFOUND_TEXT = 
  "C'est bizarre... Il semble que le fichier que vous avez selectionné n'existe pas !";

$FILEMANAGER_RESOURCE_ERROR_TITLE = 
  "$SOFTWARE_TITLE: Gestionnaire de Fichiers - Erreur de Ressource";
$FILEMANAGER_RESOURCE_ERROR_TEXT = 
  "Une erreur s'est présentée en essayant d'executer une commande de __RESOURCE__.
   Une description de cette erreur est donnée ci-dessous:";

$FILEMANAGER_SOURCE_TARGET_IDENTICAL_ERROR = 
  "Le fichier cible \"__TARGET__\" que vous avez spécifié est identique au 
   fichier source \"__SOURCE__\" que vous avez selectionné!  Oups.";

$FILEMANAGER_NO_CLOBBER_ERROR = 
  "Le fichier source \"__SOURCE__\" que vous avez selectionné est un
   répertoire. Cependant, le fichier cible \"__TARGET__\" que vous avez
   spécifié est un fichier existant. On ne peut pas remplacer un fichier
   existant avec un répertoire. Merci de bien vouloir enlever d'abord le
   fichier existant.";

$FILEMANAGER_FILE_IN_TARGET_PATH_ERROR = 
  "Le chemin cible \"__TARGET__\" que vous avez spécifié inclus un composant 
   sous-chemin \"__SUBPATH__\" qui est en fichier existant.  Afin de compléter 
   votre demande, vous devez enlever le fichier existant.";

$FILEMANAGER_FILETYPE_INVALID_ERROR = 
  "Le fichier de source \"__SOURCE__\" que vous avez selectionné est un
   __LCTYPE__. C'est un fichier invalide pour ce genre d'outil.";

$FILEMANAGER_PERMISSION_DENIED = 
  "Autorisation à accès/modifier le chemin \"__PATH__\" est nié."; 

$FILEMANAGER_EMPTY_FIELD_ERROR = 
  "Vous n'avez pas donné une valeur du __NAME__."; 

