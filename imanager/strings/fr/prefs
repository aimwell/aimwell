# file manager preferences

$PREFS_FILEMANAGER_TEXT = "Préférences de Gestionnaire de Fichiers";

$PREFS_FILEMANAGER_DENIED_TEXT = 
  "Votre nom de l'utilisateur n'inclut pas de doits de 'ftp'.  Donc
   vous ne serez pas autorisé à accéder les préférences de Gestionnaire de
   Fichiers.";

$PREFS_HIDE_ENTRIES_THAT_BEGIN_WITH_A_DOT_TEXT = 
  "Cacher les entrées de répertoire qui ont des noms commencant avec un point (.)";
$PREFS_HIDE_ENTRIES_THAT_BEGIN_WITH_A_DOT_HELP = 
  "Listages de Répertoire du Gestionnaire de Fichiers peuvent inclure les
   entrées qui ont les noms commencant avec un point (.) ou ignorer-les.
   Fichiers qui commencent avec un point sont parfois classifié comme 'fichiers
   cachés' et ils sont normalement créés et gestionnés maintained par applis.
   Par défaut, entrées de répertoire qui ont les noms commencant avec un point
   ne sont pas montrées.";

$PREFS_CONFIRM_FILE_REMOVE_TEXT = "Confirmer Enlever Fichier";
