##############################################################################
# security preferences

$PREFS_SECURITY_TEXT = "Préférences de Sécurité";

$PREFS_SECURITY_FORCE_HTTPS_TEXT = "Forcer une Connexion Sécurisé";
$PREFS_SECURITY_FORCE_HTTPS_HELP = 
  "Vous pouvez forcer toutes les demandes chiffrées (non-protocole SSL) à faire
   suivre à une session de protocole SSL chiffrée.  Bien sûr, forçant les 
   connexions SSLs vous demande de posséder un protocole SSL installé sur votre
   Serveur Virtuel.  Par défaut, les sessions non-protocole SLLs ne sont pas
   permises.";
$PREFS_SECURITY_FORCE_HTTPS_YES = "Forcer Connexions de Protocole-SSLs";
$PREFS_SECURITY_FORCE_HTTPS_NO = "Permettre les Connexions Non-protocole SSLs";

$PREFS_SECURITY_IP_CHECK_TEXT = 
  "Démarrer Authentification de Nom d'Hôte (Contrôle d'adresse IP)";
$PREFS_SECURITY_IP_CHECK_HELP = 
  "L'une des tâches exécutée est une comparaison de Noms d'Hôtes (ou adresses
   IPs) entre sessions subséquent. Cette comparaison empêche un groupe non
   autorisé d' utiliser votre clez d'authenification à n'importe quelle machine
   sauf à partir de lequel la session a commençée. Cependendant, des
   utilisateurs peuvent se connecter a Internet derrière un proxy où le
   nom d'hôte (ou adresse IP) est dynamique de session en session (consideration
   spéciale pour les utilisateurs d'AOL). Par défaut, l'authentification de nom
   d'hôte (contrôle adresse IP) est toujours accompli.";
$PREFS_SECURITY_IP_CHECK_YES = "Démarrer Contrôle d'adresse IP";
$PREFS_SECURITY_IP_CHECK_NO = "Désactiver Contrôle d'adresse IP";

