$VIRTMAPS_NO_MAPPINGS_EXIST = 
  "Il semble que votre fichier 'virtmaps' n'inclut aucune adresse virtuelle.
   Veuillez utiliser l'utilitaire 'Ajouter Virtmap' afin d'ajouter des
   nouvelles adresses virtuels.";

$VIRTMAPS_SELECT_TITLE = "Sélectionner Virtmaps";
$VIRTMAPS_SELECT_HELP = 
  "Veuillez utiliser la formulaire suivante afin de sélectionner une ou plusieurs virtmaps.";

$VIRTMAPS_OVERVIEW_HELP_TEXT = 
  "Spécifiez une adresse e-mail virtuel (à gauche) pointant sur la vraie
   adresse e-mail vraie (à droite).  L'adresse e-mail virtuelle peut être un
   e-mail entier (utilisateur\@domaine.name) ou simplement un nom d'hôte
   (domaine.name). Si vous désignez un nom d'hôte, l'e-mail envoyée à ce nom
   sera redirigée vers la vraie adresse e-mail que vous avez spécifiée, si
   aucune application d'adresse précédentes de ce nom de domaine ne
   correspond pas.  Cette entité est utile comme 'catch-all' pour les
   e-mails envoyés à n'importe quelle adresse d'un nom d'hôte donné.";

$VIRTMAPS_EXAMPLES_HELP_TEXT_1 = "Considérer les exemples suivantes:";
$VIRTMAPS_EXAMPLES_HELP_TEXT_2 = 
  "  Virtual E-mail Address                Real E-mail Address
  ----------------------                ------------------
  webmaster\@virtualhost1.com            utilisateur\@mail.dialup.com
  utilisateur\@virtualhost1.com                 localuser1
  hôtevirtuel1.com                       utilisateur\@isp.com
  # Noter: \"Utilisateur Inconnue\" courrier à hôtevirtuel1.nom est envoyé à utilisateur\@isp.com
 
  webmaster\@virtualhost2.com            localuser2
  utilisateur\@virtualhost2.com          jean\@aol.com
  foobar\@virtualhost2.com               jacques\@hotmail.com
  hôtevirtuel2.com                       localuser2";

$VIRTMAPS_VIRTUAL_EMAIL_ADDRESS = "Adresse E-mail Virtuel";
$VIRTMAPS_REAL_EMAIL_ADDRESS = "Adresse E-mail Vraie";

$VIRTMAPS_VIRTMAP_PLACEMENT = "Placement de Virtmap";
$VIRTMAPS_VIRTMAP_PLACEMENT_APPEND = "ajouter a la fin du fichier";
$VIRTMAPS_VIRTMAP_PLACEMENT_INSERT = "insérer aprés __VIRTMAP__";

