$PASSWD_WARNING_DUPLICATE_LOGIN =
  "Avertissement: Une occurrence double de __LOGIN__ trouvée dans le fichier de mot de passe...  Passez.";

$PASSWD_WARNING_ROOT_UID =
  "Avertissement: Profil de l'Utilisateur (root) a l'identificateur __UID__ ne correspond pas à l'identificateur de 0.";
$PASSWD_WARNING_ROOT_GID =
  "Avertissement: Profil de l'Utilisateur (root) a un indentificateur de groupe de __GID__ ne correspond pas à l'identificateur de groupe de 0.";
$PASSWD_WARNING_NONROOT_UID =
  "Avertissement: Profil de l'Utilisateur (__LOGIN__) a l'identificateur de __UID__ ne correspond pas à l'identificateur de __EUID__.";
$PASSWD_WARNING_NONROOT_GID =
  "Avertissement: Profil de l'Utilisateur dans (__LOGIN__) a l'identificateur de groupe de __GID__ ne
   correspond pas à l'identificateur de groupe de __EGID__.";

$PASSWD_RESOURCE_ERROR_TITLE = "$SOFTWARE_TITLE: Erreur Ressource de Fichier de Mot de Passe";
$PASSWD_RESOURCE_ERROR_TEXT = 
  "Une erreur s'est présentée en tentant d'éxecuter une commande de mot de passe.
   Une description de l'erreur vous est donnée ci-dessous:";

##############################################################################
# eof

1;
