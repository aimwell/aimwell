$ALIASES_NO_MAPPINGS_EXIST = 
  "Il semble que votre fichier alias ne comporte aucune definition d'alias.  
Merci de bien vouloir utiliser l'utilitaire 'Ajout d'alias' pour en créer un.";

$ALIASES_SELECT_TITLE = "Selectionez l'Alias";
$ALIASES_SELECT_HELP = 
  "Merci de bien vouloir utiliser le formulaire suivant afin de choisir un ou plusieurs alias.";

$ALIASES_OVERVIEW_HELP_TEXT = 
  "Un alias d'e-mail est simplement un transfert de courrier électronique.  
   Chaque alias que vous spécifiez transfere l'e-mail suivant la définition de
   l'Alias. La définition peut être une ou plusieurs adresses e-mails 
   (plusieurs adresses doivent être séparées par des virgules), sinon elle peut 
   être une définition complexe qui dirige votre service de courrier à 
   livrer un message vers un mailing-list ou un programme tel qu'un répondeur.";

$ALIASES_EXAMPLES_HELP_TEXT_1 = "Considérez les exemples suivants:";
$ALIASES_EXAMPLES_HELP_TEXT_2 = 
  "  Alias d'e-mail                 Alias Definition
  ----------------------         ----------------------------
  webmaster                      utilisateurlocal1
  chiffre                        utilisateurlocal2, utilisateurlocal3
  pierre                         utilisateur\@aol.com
  fonctionnels                   :inclusion:/etc/mail/staff_address.list
  information                    webmestre, \"|/usr/bin/autoreply -f webmaster -m /etc/mail/faq_message.txt -a info\"";

$ALIASES_ALIAS_NAME = "Alias d'E-mail";
$ALIASES_ALIAS_VALUE = "Définition d'Alias";

$ALIASES_ALIAS_PLACEMENT = "Placement d'Alias";
$ALIASES_ALIAS_PLACEMENT_APPEND = "ajoutez à la fin du fichier";
$ALIASES_ALIAS_PLACEMENT_INSERT = "insérez après __ALIAS__";

$ALIASES_ADD_HELP_TEXT = 
  "Merci de bien vouloir utiliser la formulaire suivant afin d'ajouter des nouvelles définitions d'alias.";
$ALIASES_ADD_SUBMIT_TEXT = $SUBMIT_STRING;

$ALIASES_EDIT_HELP_TEXT = 
  "Merci de bien vouloir utiliser la formulaire suivant afin de modifier les alias sélectionnés.";
$ALIASES_EDIT_SUBMIT_TEXT = "Soumettez des changements";

$ALIASES_REMOVE_HELP_TEXT = 
  "Etes-vous certain de vouloir enlever l'alias énuméré ci-dessous?";
$ALIASES_REMOVE_SUBMIT_TEXT = "Oui, enlevez l'alias ci-dessus";

$ALIASES_SORT_BY_NAME = "Triez les alias par nom";
$ALIASES_SORT_BY_ORDER = "Triez par Ordre de Fichier";

$ALIASES_SUCCESS_ADD_TEXT = "Votre demande 'Ajout d'alias' a été traitée.";
$ALIASES_SUCCESS_EDIT_TEXT = "Votre demande 'Édit d'alias' a été traitée.";
$ALIASES_SUCCESS_REMOVE_TEXT =
  "Votre demande 'Enleve d'Alias' a été traitée.";

